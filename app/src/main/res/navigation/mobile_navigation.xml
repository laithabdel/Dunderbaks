<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.android.dunderbaks.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_tap_list"
        android:name="com.android.dunderbaks.ui.dashboard.TapListFragment"
        android:label="Tap List"
        tools:layout="@layout/fragment_tap_list" />

    <fragment
        android:id="@+id/navigation_menu"
        android:name="com.android.dunderbaks.ui.notifications.MenuFragment"
        android:label="Menu"
        tools:layout="@layout/fragment_menu" />
</navigation>